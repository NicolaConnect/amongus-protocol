{
  "name": "amongus-protocol",
  "version": "1.1.2",
  "description": "A recreated Among Us protocol implemented in Typescript.",
  "main": "js/index.js",
  "scripts": {
    "build": "tsc -p .",
    "load": "node --loader ts-node/esm",
    "test": "npm run load ts/test/main.ts",
    "test:search": "npm run load ts/test/search.ts",
    "test:knowledge": "npm run load ts/test/knowledge.ts",
    "test:server": "npm run load ts/test/server.ts",
    "docs": "typedoc --out docs --mode modules --excludePrivate --excludeNotExported ts/lib",
    "cmd": "npm run load ts/test/cmdline.ts",
    "parse": "npm run load ts/script/parse.ts",
    "compose": "npm run load ts/script/compose.ts",
    "view": "npm run load ts/script/view.ts",
    "postinstall": "npm run build"
  },
  "type": "module",
  "keywords": [],
  "author": "weakeyes",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^14.11.10",
    "ts-node": "github:TypeStrong/ts-node#master",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  }
}
